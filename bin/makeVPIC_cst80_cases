#!/bin/bash

################################################################################
# Build the optimized Intel case for the master branch without vector support.

ARCH=CST80_MASTER_CLE_INT_OPT_NONE
BRANCH=master_wdn_v8
/usr/bin/time -p bin/makeVPIC_cst80 $ARCH $BRANCH 8 >& makeVPIC_cst80.log
mv makeVPIC_cst80.log build/$ARCH/vpic

################################################################################
# Build the optimized Intel cases for the master branch with V4 support.

ARCH=CST80_MASTER_CLE_INT_OPT_V4_PORT
BRANCH=master_wdn_v8
/usr/bin/time -p bin/makeVPIC_cst80 $ARCH $BRANCH 8 >& makeVPIC_cst80.log
mv makeVPIC_cst80.log build/$ARCH/vpic

ARCH=CST80_MASTER_CLE_INT_OPT_V4_SSE
BRANCH=master_wdn_v8
/usr/bin/time -p bin/makeVPIC_cst80 $ARCH $BRANCH 8 >& makeVPIC_cst80.log
mv makeVPIC_cst80.log build/$ARCH/vpic

ARCH=CST80_MASTER_CLE_INT_OPT_V4_AVX2
BRANCH=master_wdn_v8
/usr/bin/time -p bin/makeVPIC_cst80 $ARCH $BRANCH 8 >& makeVPIC_cst80.log
mv makeVPIC_cst80.log build/$ARCH/vpic

################################################################################
# Build the optimized Intel cases for the master branch with V8 support.

ARCH=CST80_MASTER_CLE_INT_OPT_V8_PORT
BRANCH=master_wdn_v8
/usr/bin/time -p bin/makeVPIC_cst80 $ARCH $BRANCH 8 >& makeVPIC_cst80.log
mv makeVPIC_cst80.log build/$ARCH/vpic

ARCH=CST80_MASTER_CLE_INT_OPT_V8_AVX2
BRANCH=master_wdn_v8
/usr/bin/time -p bin/makeVPIC_cst80 $ARCH $BRANCH 8 >& makeVPIC_cst80.log
mv makeVPIC_cst80.log build/$ARCH/vpic

################################################################################
# Build the optimized GNU case for the master branch without vector support.

ARCH=CST80_MASTER_CLE_GNU_OPT_NONE
BRANCH=master_wdn_v8
/usr/bin/time -p bin/makeVPIC_cst80 $ARCH $BRANCH 8 >& makeVPIC_cst80.log
mv makeVPIC_cst80.log build/$ARCH/vpic

################################################################################
# Build the optimized GNU cases for the master branch with V4 support.

ARCH=CST80_MASTER_CLE_GNU_OPT_V4_PORT
BRANCH=master_wdn_v8
/usr/bin/time -p bin/makeVPIC_cst80 $ARCH $BRANCH 8 >& makeVPIC_cst80.log
mv makeVPIC_cst80.log build/$ARCH/vpic

ARCH=CST80_MASTER_CLE_GNU_OPT_V4_SSE
BRANCH=master_wdn_v8
/usr/bin/time -p bin/makeVPIC_cst80 $ARCH $BRANCH 8 >& makeVPIC_cst80.log
mv makeVPIC_cst80.log build/$ARCH/vpic

ARCH=CST80_MASTER_CLE_GNU_OPT_V4_AVX2
BRANCH=master_wdn_v8
/usr/bin/time -p bin/makeVPIC_cst80 $ARCH $BRANCH 8 >& makeVPIC_cst80.log
mv makeVPIC_cst80.log build/$ARCH/vpic

################################################################################
# Build the optimized GNU cases for the master branch with V8 support.

ARCH=CST80_MASTER_CLE_GNU_OPT_V8_PORT
BRANCH=master_wdn_v8
/usr/bin/time -p bin/makeVPIC_cst80 $ARCH $BRANCH 8 >& makeVPIC_cst80.log
mv makeVPIC_cst80.log build/$ARCH/vpic

ARCH=CST80_MASTER_CLE_GNU_OPT_V8_AVX2
BRANCH=master_wdn_v8
/usr/bin/time -p bin/makeVPIC_cst80 $ARCH $BRANCH 8 >& makeVPIC_cst80.log
mv makeVPIC_cst80.log build/$ARCH/vpic
