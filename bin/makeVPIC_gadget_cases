#!/bin/bash

################################################################################
# Build the optimized Intel case for the master branch without vector support.

ARCH=GADGET_MASTER_CLE_INT_OPT_NONE_KNL
BRANCH=master_wdn_v8
/usr/bin/time -p bin/makeVPIC_gadget $ARCH $BRANCH 8 >& makeVPIC_gadget.log
mv makeVPIC_gadget.log build/$ARCH/vpic

################################################################################
# Build the optimized Intel cases for the master branch with V4 support.

ARCH=GADGET_MASTER_CLE_INT_OPT_V4_PORT_KNL
BRANCH=master_wdn_v8
/usr/bin/time -p bin/makeVPIC_gadget $ARCH $BRANCH 8 >& makeVPIC_gadget.log
mv makeVPIC_gadget.log build/$ARCH/vpic

ARCH=GADGET_MASTER_CLE_INT_OPT_V4_SSE_KNL
BRANCH=master_wdn_v8
/usr/bin/time -p bin/makeVPIC_gadget $ARCH $BRANCH 8 >& makeVPIC_gadget.log
mv makeVPIC_gadget.log build/$ARCH/vpic

ARCH=GADGET_MASTER_CLE_INT_OPT_V4_AVX2_KNL
BRANCH=master_wdn_v8
/usr/bin/time -p bin/makeVPIC_gadget $ARCH $BRANCH 8 >& makeVPIC_gadget.log
mv makeVPIC_gadget.log build/$ARCH/vpic

