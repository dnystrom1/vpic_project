#!/bin/bash

################################################################################
# Build the optimized Intel case for the master branch without vector support.

ARCH=GRIZZLY_PTH_INT_OPT_NONE
BRANCH=knl_open_science
/usr/bin/time -p bin/makeVPIC_grizzly $ARCH $BRANCH 32 >& makeVPIC_grizzly.log
mv makeVPIC_grizzly.log build/$ARCH/vpic

################################################################################
# Build the optimized Intel cases for the master branch with V4 support.

ARCH=GRIZZLY_PTH_INT_OPT_V4_PORT
BRANCH=knl_open_science
/usr/bin/time -p bin/makeVPIC_grizzly $ARCH $BRANCH 32 >& makeVPIC_grizzly.log
mv makeVPIC_grizzly.log build/$ARCH/vpic

