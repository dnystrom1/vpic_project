#!/bin/bash

################################################################################
# Build the optimized Intel case for the master branch without vector support.

ARCH=TRINITITE_MASTER_CLE_INT_OPT_NONE_HSW
BRANCH=master_wdn_v8
/usr/bin/time -p bin/makeVPIC_trinitite $ARCH $BRANCH 8 >& makeVPIC_trinitite.log
mv makeVPIC_trinitite.log build/$ARCH/vpic

################################################################################
# Build the optimized Intel cases for the master branch with V4 support.

ARCH=TRINITITE_MASTER_CLE_INT_OPT_V4_PORT_HSW
BRANCH=master_wdn_v8
/usr/bin/time -p bin/makeVPIC_trinitite $ARCH $BRANCH 8 >& makeVPIC_trinitite.log
mv makeVPIC_trinitite.log build/$ARCH/vpic

ARCH=TRINITITE_MASTER_CLE_INT_OPT_V4_SSE_HSW
BRANCH=master_wdn_v8
/usr/bin/time -p bin/makeVPIC_trinitite $ARCH $BRANCH 8 >& makeVPIC_trinitite.log
mv makeVPIC_trinitite.log build/$ARCH/vpic

ARCH=TRINITITE_MASTER_CLE_INT_OPT_V4_AVX2_HSW
BRANCH=master_wdn_v8
/usr/bin/time -p bin/makeVPIC_trinitite $ARCH $BRANCH 8 >& makeVPIC_trinitite.log
mv makeVPIC_trinitite.log build/$ARCH/vpic

################################################################################
# Build the optimized Intel cases for the master branch with V8 support.

ARCH=TRINITITE_MASTER_CLE_INT_OPT_V8_PORT_HSW
BRANCH=master_wdn_v8
/usr/bin/time -p bin/makeVPIC_trinitite $ARCH $BRANCH 8 >& makeVPIC_trinitite.log
mv makeVPIC_trinitite.log build/$ARCH/vpic

ARCH=TRINITITE_MASTER_CLE_INT_OPT_V8_AVX2_HSW
BRANCH=master_wdn_v8
/usr/bin/time -p bin/makeVPIC_trinitite $ARCH $BRANCH 8 >& makeVPIC_trinitite.log
mv makeVPIC_trinitite.log build/$ARCH/vpic

################################################################################
# Build the optimized GNU case for the master branch without vector support.

ARCH=TRINITITE_MASTER_CLE_GNU_OPT_NONE_HSW
BRANCH=master_wdn_v8
/usr/bin/time -p bin/makeVPIC_trinitite $ARCH $BRANCH 8 >& makeVPIC_trinitite.log
mv makeVPIC_trinitite.log build/$ARCH/vpic

################################################################################
# Build the optimized GNU cases for the master branch with V4 support.

ARCH=TRINITITE_MASTER_CLE_GNU_OPT_V4_PORT_HSW
BRANCH=master_wdn_v8
/usr/bin/time -p bin/makeVPIC_trinitite $ARCH $BRANCH 8 >& makeVPIC_trinitite.log
mv makeVPIC_trinitite.log build/$ARCH/vpic

ARCH=TRINITITE_MASTER_CLE_GNU_OPT_V4_SSE_HSW
BRANCH=master_wdn_v8
/usr/bin/time -p bin/makeVPIC_trinitite $ARCH $BRANCH 8 >& makeVPIC_trinitite.log
mv makeVPIC_trinitite.log build/$ARCH/vpic

ARCH=TRINITITE_MASTER_CLE_GNU_OPT_V4_AVX2_HSW
BRANCH=master_wdn_v8
/usr/bin/time -p bin/makeVPIC_trinitite $ARCH $BRANCH 8 >& makeVPIC_trinitite.log
mv makeVPIC_trinitite.log build/$ARCH/vpic

################################################################################
# Build the optimized GNU cases for the master branch with V8 support.

ARCH=TRINITITE_MASTER_CLE_GNU_OPT_V8_PORT_HSW
BRANCH=master_wdn_v8
/usr/bin/time -p bin/makeVPIC_trinitite $ARCH $BRANCH 8 >& makeVPIC_trinitite.log
mv makeVPIC_trinitite.log build/$ARCH/vpic

ARCH=TRINITITE_MASTER_CLE_GNU_OPT_V8_AVX2_HSW
BRANCH=master_wdn_v8
/usr/bin/time -p bin/makeVPIC_trinitite $ARCH $BRANCH 8 >& makeVPIC_trinitite.log
mv makeVPIC_trinitite.log build/$ARCH/vpic
