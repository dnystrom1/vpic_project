${aprun} -n   34 -N 1 -c 4 --cpu_bind=threads ${voc} ${vtime} ${vnuma} ${xpre}${prob}_ddr_nn_0001_nppn_034_ntpp_001.Linux --tpp 1 >& ${prob}_ddr_nn_0001_nppn_034_ntpp_001.log
