########################################################################################################################################################
# MPI only, 2 ranks/core.

srun -n 1088 -N 8 -c 2 --cpu_bind=threads ${vtime} ${vnuma} ${prob}_ddr_nn_0008_nppn_136_ntpp_001.Linux --tpp 1 >& ${prob}_ddr_nn_0008_nppn_136_nrpc_002.log
srun -n 1024 -N 8 -c 2 --cpu_bind=threads ${vtime} ${vnuma} ${prob}_ddr_nn_0008_nppn_128_ntpp_001.Linux --tpp 1 >& ${prob}_ddr_nn_0008_nppn_128_nrpc_002.log

