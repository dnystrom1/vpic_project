srun -n  136 -N 2 -c 4 --cpu_bind=threads ${vtime} ${vnuma} ${prob}_ddr_nn_0002_nppn_068_ntpp_001.Linux --tpp 1 >& ${prob}_ddr_nn_0002_nppn_068_nrpc_001.log
srun -n  128 -N 2 -c 4 --cpu_bind=threads ${vtime} ${vnuma} ${prob}_ddr_nn_0002_nppn_064_ntpp_001.Linux --tpp 1 >& ${prob}_ddr_nn_0002_nppn_064_nrpc_001.log

srun -n   68 -N 1 -c 4 --cpu_bind=threads ${vtime} ${vnuma} ${prob}_ddr_nn_0001_nppn_068_ntpp_001.Linux --tpp 1 >& ${prob}_ddr_nn_0001_nppn_068_nrpc_001.log
srun -n   64 -N 1 -c 4 --cpu_bind=threads ${vtime} ${vnuma} ${prob}_ddr_nn_0001_nppn_064_ntpp_001.Linux --tpp 1 >& ${prob}_ddr_nn_0001_nppn_064_nrpc_001.log
