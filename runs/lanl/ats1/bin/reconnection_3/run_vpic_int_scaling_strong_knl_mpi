srun -n   68 -N 1 -c 1 --cpu_bind=threads ${vtime} ${vnuma} ${prob}_hbm_nn_0001_nppn_068_ntpp_001.Linux --tpp 1 >& ${prob}_hbm_nn_0001_nppn_068_nrpc_004.log
srun -n   64 -N 1 -c 1 --cpu_bind=threads ${vtime} ${vnuma} ${prob}_hbm_nn_0001_nppn_064_ntpp_001.Linux --tpp 1 >& ${prob}_hbm_nn_0001_nppn_064_nrpc_004.log
