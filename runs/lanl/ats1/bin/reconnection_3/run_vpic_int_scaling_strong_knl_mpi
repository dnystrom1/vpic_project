#!/bin/bash

########################################################################################################################################################
# Configure modules.

module load friendly-testing
module load sandbox
module swap intel intel/17.0.4
module swap cray-mpich cray-mpich/7.5.4
module swap craype-haswell craype-mic-knl

module list

########################################################################################################################################################
# Preliminary stuff.

prob=reconnection

vtime='/usr/bin/time -p'
vnuma='numactl --preferred=1'

########################################################################################################################################################
# Large DDR problem, strong scaled across 8 nodes.

########################################################################################################################################################
# MPI only, 4 ranks/core.

srun -n 2176 -N 8 -c 1 --cpu_bind=threads ${vtime} ${vnuma} ${prob}_ddr_nn_0008_nppn_272_ntpp_001.Linux --tpp 1 >& ${prob}_ddr_nn_0008_nppn_272_nrpc_004.log
srun -n 2048 -N 8 -c 1 --cpu_bind=threads ${vtime} ${vnuma} ${prob}_ddr_nn_0008_nppn_256_ntpp_001.Linux --tpp 1 >& ${prob}_ddr_nn_0008_nppn_256_nrpc_004.log

srun -n 1088 -N 4 -c 1 --cpu_bind=threads ${vtime} ${vnuma} ${prob}_ddr_nn_0004_nppn_272_ntpp_001.Linux --tpp 1 >& ${prob}_ddr_nn_0004_nppn_272_nrpc_004.log
srun -n 1024 -N 4 -c 1 --cpu_bind=threads ${vtime} ${vnuma} ${prob}_ddr_nn_0004_nppn_256_ntpp_001.Linux --tpp 1 >& ${prob}_ddr_nn_0004_nppn_256_nrpc_004.log

srun -n  544 -N 2 -c 1 --cpu_bind=threads ${vtime} ${vnuma} ${prob}_ddr_nn_0002_nppn_272_ntpp_001.Linux --tpp 1 >& ${prob}_ddr_nn_0002_nppn_272_nrpc_004.log
srun -n  512 -N 2 -c 1 --cpu_bind=threads ${vtime} ${vnuma} ${prob}_ddr_nn_0002_nppn_256_ntpp_001.Linux --tpp 1 >& ${prob}_ddr_nn_0002_nppn_256_nrpc_004.log

srun -n  272 -N 1 -c 1 --cpu_bind=threads ${vtime} ${vnuma} ${prob}_ddr_nn_0001_nppn_272_ntpp_001.Linux --tpp 1 >& ${prob}_ddr_nn_0001_nppn_272_nrpc_004.log
srun -n  256 -N 1 -c 1 --cpu_bind=threads ${vtime} ${vnuma} ${prob}_ddr_nn_0001_nppn_256_ntpp_001.Linux --tpp 1 >& ${prob}_ddr_nn_0001_nppn_256_nrpc_004.log
srun -n  136 -N 1 -c 1 --cpu_bind=threads ${vtime} ${vnuma} ${prob}_ddr_nn_0001_nppn_136_ntpp_001.Linux --tpp 1 >& ${prob}_ddr_nn_0001_nppn_136_nrpc_004.log
srun -n  128 -N 1 -c 1 --cpu_bind=threads ${vtime} ${vnuma} ${prob}_ddr_nn_0001_nppn_128_ntpp_001.Linux --tpp 1 >& ${prob}_ddr_nn_0001_nppn_128_nrpc_004.log
srun -n   68 -N 1 -c 1 --cpu_bind=threads ${vtime} ${vnuma} ${prob}_ddr_nn_0001_nppn_068_ntpp_001.Linux --tpp 1 >& ${prob}_ddr_nn_0001_nppn_068_nrpc_004.log
srun -n   64 -N 1 -c 1 --cpu_bind=threads ${vtime} ${vnuma} ${prob}_ddr_nn_0001_nppn_064_ntpp_001.Linux --tpp 1 >& ${prob}_ddr_nn_0001_nppn_064_nrpc_004.log
srun -n   32 -N 1 -c 1 --cpu_bind=threads ${vtime} ${vnuma} ${prob}_ddr_nn_0001_nppn_032_ntpp_001.Linux --tpp 1 >& ${prob}_ddr_nn_0001_nppn_032_nrpc_004.log
srun -n   16 -N 1 -c 1 --cpu_bind=threads ${vtime} ${vnuma} ${prob}_ddr_nn_0001_nppn_016_ntpp_001.Linux --tpp 1 >& ${prob}_ddr_nn_0001_nppn_016_nrpc_004.log
srun -n    8 -N 1 -c 1 --cpu_bind=threads ${vtime} ${vnuma} ${prob}_ddr_nn_0001_nppn_008_ntpp_001.Linux --tpp 1 >& ${prob}_ddr_nn_0001_nppn_008_nrpc_004.log
