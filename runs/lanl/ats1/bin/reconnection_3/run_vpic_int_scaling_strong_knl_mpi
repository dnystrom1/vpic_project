srun -n    1 -N 1 -c 4 --cpu_bind=threads ${vtime} ${vnuma} ${prob}_ddr_nn_0001_nppn_001_ntpp_001.Linux --tpp 1 >& ${prob}_ddr_nn_0001_nppn_001_nrpc_001.log

########################################################################################################################################################
# Small HBM problem, strong scaled across 1 node.

########################################################################################################################################################
# MPI only, 4 ranks/core.

srun -n  272 -N 1 -c 1 --cpu_bind=threads ${vtime} ${vnuma} ${prob}_hbm_nn_0001_nppn_272_ntpp_001.Linux --tpp 1 >& ${prob}_hbm_nn_0001_nppn_272_nrpc_004.log
srun -n  256 -N 1 -c 1 --cpu_bind=threads ${vtime} ${vnuma} ${prob}_hbm_nn_0001_nppn_256_ntpp_001.Linux --tpp 1 >& ${prob}_hbm_nn_0001_nppn_256_nrpc_004.log
