srun -n  544 -N 8 -c 4 --cpu_bind=threads ${vtime} ${vnuma} ${prob}_ddr_nn_0008_nppn_068_ntpp_001.Linux --tpp 1 >& ${prob}_ddr_nn_0008_nppn_068_nrpc_001.log
