srun -n    8 -N 1 -c 1 --cpu_bind=threads ${vtime} ${vnuma} ${prob}_hbm_nn_0001_nppn_008_ntpp_001.Linux --tpp 1 >& ${prob}_hbm_nn_0001_nppn_008_nrpc_004.log
